  },
  "homepage": "https://github.com/eeue56/elm-documentation-parser#readme",
  "devDependencies": {
    "@types/node": "^8.0.17",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2",
    "typescript-formatter": "^5.2.0"
  },
  "dependencies": {
    "@types/fs-extra": "^5.0.0",
    "@types/glob": "^5.0.35",
    "es6-promise": "^4.1.1",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2"
  }
